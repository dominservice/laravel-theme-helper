{
  "name": "dominservice/laravel-theme-helper",
  "description": "Merge default config with dynamic settings, and optimize.",
  "type": "library",
  "keywords": ["laravel", "theme", "seo", "meta", "application/ld+json", "json-ld"],
  "homepage": "https://github.com/dominservice/laravel-theme-helper",
  "license": "MIT",
  "authors": [
    {
      "name": "DSO-IT Mateusz Domin",
      "email": "biuro@dso.biz.pl"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "laravel/framework": "^10.0|^11.0|^12",
    "spatie/schema-org": "^3.23"
  },
  "autoload": {
    "files": [
      "src/Helpers/helpers.php"
    ],
    "psr-4": {
      "Dominservice\\LaravelThemeHelper\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dominservice\\LaravelThemeHelper\\ServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
